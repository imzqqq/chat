[Unit]
Description=Chat->Flow/Twitter Bot
Wants=network.target
StartLimitIntervalSec=0

[Service]
User=root
WorkingDirectory=/home/imzqqq/workspace/chat/server/chat/python/bridges/mycete
ExecStart=/home/imzqqq/workspace/chat/server/chat/python/bridges/mycete/mycete

Type=simple
Nice=15
Restart=always
RestartSec=8
NoNewPrivileges=true
ProtectSystem=strict
#ProtectHome=true
ProtectKernelTunables=true
SystemCallArchitectures=native
SystemCallFilter=~@chown @clock @cpu-emulation @debug @keyring @module @mount @obsolete @privileged @swap @sync @raw-io
CPUQuota=50%
PrivateTmp=true
ProtectControlGroups=yes
## options below won't work in --user context. comment them out for --user to work.
#PrivateDevices=true
#ProtectKernelModules=true

[Install]
WantedBy=multi-user.target
